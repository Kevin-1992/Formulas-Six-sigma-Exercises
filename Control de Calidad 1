import matplotlib.pyplot as plt
import statistics

#Importing CVV File from drive
from google.colab import drive
drive.mount('/content/drive')

data_15 = pd.read_csv('/content/drive/My Drive/Tareas USAC/DATA_15.csv',header=None)

